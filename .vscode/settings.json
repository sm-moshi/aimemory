{
	"[ignore]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"chat.editor.wordWrap": "on",
	"cursor.composer.collapsePaneInputBoxPills": true,
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"cursor.composer.shouldQueueWhenGenerating": true,
	"debug.showVariableTypes": true,
	"dependi.npm.unstableFilter": "IncludeAlways",
	"dependi.vulnerability.ghsa.enabled": true,
	"diffEditor.experimental.showMoves": true,
	"diffEditor.experimental.useTrueInlineView": true,
	"editor.guides.bracketPairs": true,
	"editor.inlineSuggest.syntaxHighlightingEnabled": true,
	"editor.semanticHighlighting.enabled": true,
	"errorLens.codeLensEnabled": true,
	"files.associations": {
		"*.cjs": "javascript",
		"*.css": "tailwindcss"
	},
	"files.autoSaveDelay": 2500,
	"gitlens.defaultDateFormat": "YYYY-MM-DD HH:mm:ss",
	"gitlens.gitkraken.cli.integration.enabled": true,
	"js/ts.implicitProjectConfig.checkJs": true,
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"json.schemaDownload.enable": true,
	"problems.showCurrentInStatus": true,
	"problems.visibility": true,
	"rewrap.autoWrap.enabled": true,
	"rewrap.wrappingColumn": 120,
	"search.exclude": {
		"**/.vscode-test": true,
		"**/coverage": true,
		"**/dist": true,
		"**/node_modules": true
	},
	"files.watcherExclude": {
		"**/.git/objects/**": true,
		"**/.git/subtree-cache/**": true,
		"**/.hg/store/**": true,
		"**/node_modules/**": true
	},
	"search.quickOpen.includeHistory": true,
	"sonarlint.connectedMode.project": {
		"connectionId": "sm-moshi",
		"projectKey": "sm-moshi_aimemory"
	},
	"testing.coverageToolbarEnabled": true,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"typescript.experimental.useTsgo": false,
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.implementationsCodeLens.showOnInterfaceMethods": true,
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.preferGoToSourceDefinition": true,
	"typescript.referencesCodeLens.enabled": true,
	"typescript.suggest.autoImports": true,
	"typescript.suggest.includeAutomaticOptionalChainCompletions": true,
	"typescript.suggest.paths": true,
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"versionlens.suggestions.showOnStartup": true,
	"versionlens.suggestions.showSuggestionsStats": true,
	"workbench.commandPalette.experimental.suggestCommands": true,
	"workbench.commandPalette.preserveInput": true,
	"yaml.format.printWidth": 120,
}