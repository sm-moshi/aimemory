{
	"[ignore]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"biome.configurationPath": "biome.json",
	"aimemory.logLevel": "debug",
	"chat.editor.wordWrap": "on",
	"cursor.composer.collapsePaneInputBoxPills": true,
	"cursor.composer.shouldChimeAfterChatFinishes": true,
	"cursor.composer.shouldQueueWhenGenerating": true,
	"cursor.composer.shouldShowMarkdownHoverParticipantActions2": true,
	"cursor.general.enableShadowWorkspace": false,
	"debug.javascript.enableNetworkView": false,
	"debug.showVariableTypes": true,
	"dependi.npm.enabled": true,
	"dependi.npm.informPatchUpdates": true,
	"dependi.npm.lockFileEnabled": false,
	"dependi.npm.unstableFilter": "IncludeAlways",
	"dependi.vulnerability.ghsa.enabled": true,
	"diffEditor.experimental.showMoves": true,
	"diffEditor.experimental.useTrueInlineView": true,
	"editor.experimental.asyncTokenization": true,
	"editor.experimentalEditContextEnabled": true,
	"editor.foldingImportsByDefault": true,
	"editor.guides.bracketPairs": true,
	"editor.inlineSuggest.syntaxHighlightingEnabled": true,
	"editor.semanticHighlighting.enabled": true,
	"editor.wordWrapColumn": 120,
	"errorLens.codeLensEnabled": true,
	"files.associations": {
		"*.cjs": "javascript",
		"*.css": "tailwindcss"
	},
	"gitlens.ai.experimental.generateCommits.enabled": true,
	"gitlens.ai.experimental.generateRebase.enabled": true,
	"gitlens.gitkraken.cli.integration.enabled": true,
	"js/ts.implicitProjectConfig.checkJs": true,
	"js/ts.implicitProjectConfig.experimentalDecorators": true,
	"json.schemaDownload.enable": true,
	"problems.showCurrentInStatus": true,
	"problems.visibility": true,
	"rewrap.autoWrap.enabled": true,
	"rewrap.wrappingColumn": 120,
	"search.exclude": {
		"**/.vscode-test": true,
		"**/coverage": true,
		"**/dist": true,
		"**/node_modules": true
	},
	"search.quickOpen.includeHistory": true,
	"sonarlint.connectedMode.project": {
		"connectionId": "sm-moshi-org",
		"projectKey": "sm-moshi_aimemory"
	},
	"testing.coverageToolbarEnabled": true,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"typescript.experimental.expandableHover": false,
	"typescript.experimental.useTsgo": true,
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.implementationsCodeLens.showOnInterfaceMethods": true,
	"typescript.preferGoToSourceDefinition": true,
	"typescript.suggest.autoImports": true,
	"typescript.suggest.includeAutomaticOptionalChainCompletions": true,
	"typescript.suggest.paths": true,
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"versionlens.suggestions.showOnStartup": true,
	"versionlens.suggestions.showSuggestionsStats": true,
	"vsicons.presets.jsonOfficial": true,
	"workbench.commandPalette.experimental.suggestCommands": true,
	"workbench.commandPalette.history": 100,
	"workbench.commandPalette.preserveInput": true,
	"yaml.format.printWidth": 120
}
