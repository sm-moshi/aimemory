{
	"aimemory.logLevel": "debug",
	"coverage-gutters.coverageFileNames": [
		"lcov.info",
		"cov.xml",
		"coverage.xml",
		"jacoco.xml",
		"coverage.lcov"
	],
	"coverage-gutters.showGutterCoverage": true,
	"coverage-gutters.showLineCoverage": true,
	"coverage-gutters.showRulerCoverage": true,
	"debug.focusWindowOnBreak": true,
	"debug.inlineValues": "auto",
	"debug.javascript.autoAttachFilter": "smart",
	"debug.javascript.enableNetworkView": true,
	"debug.showBreakpointsInOverviewRuler": true,
	"debug.toolBarLocation": "floating",
	"dependi.npm.informPatchUpdates": true,
	"dependi.npm.lockFileEnabled": true,
	"dependi.npm.unstableFilter": "IncludeAlways",
	"dependi.vulnerability.ghsa.enabled": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.codeActionsOnSave": {
		"source.fixAll.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"evenBetterToml.formatter.alignComments": true,
	"evenBetterToml.formatter.alignEntries": true,
	"evenBetterToml.formatter.allowedBlankLines": 3,
	"extensions.ignoreRecommendations": false,
	"files.associations": {
		"*.cjs": "javascript"
	},
	"files.autoSave": "off",
	"files.watcherExclude": {
		"**/.vscode-test/**": true,
		"**/coverage/**": true,
		"**/node_modules/**": true
	},
	"git.autofetch": true,
	"git.enableSmartCommit": true,
	"git.suggestSmartCommit": true,
	"javascript.referencesCodeLens.enabled": true,
	"javascript.suggest.completeFunctionCalls": true,
	"javascript.updateImportsOnPaste.enabled": true,
	"merge-conflict.autoNavigateNextConflict.enabled": true,
	"npm-intellisense.packageSubfoldersIntellisense": true,
	"npm-intellisense.scanDevDependencies": true,
	"npm.enableRunFromFolder": true,
	"npm.fetchOnlinePackageInfo": true,
	"npm.packageManager": "pnpm",
	"scm.inputFontSize": 12,
	"search.exclude": {
		"**/.vscode-test": true,
		"**/coverage": true,
		"**/node_modules": true
	},
	"sonarlint.connectedMode.project": {
		"connectionId": "sm-moshi-org",
		"projectKey": "sm-moshi_aimemory"
	},
	// MCP Development Settings (via terminal environment)
	"terminal.integrated.env.osx": {
		"DEBUG": "mcp:*"
	},
	"typescript.experimental.expandableHover": true,
	"typescript.experimental.useTsgo": true,
	"typescript.implementationsCodeLens.enabled": true,
	"typescript.implementationsCodeLens.showOnInterfaceMethods": true,
	"typescript.inlayHints.enumMemberValues.enabled": true,
	"typescript.inlayHints.functionLikeReturnTypes.enabled": true,
	"typescript.inlayHints.parameterNames.enabled": "literals",
	"typescript.inlayHints.parameterTypes.enabled": true,
	"typescript.inlayHints.propertyDeclarationTypes.enabled": true,
	"typescript.native-preview.tsdk": "/Users/smeya/.local/share/pnpm/tsgo",
	"typescript.preferences.includePackageJsonAutoImports": "on",
	"typescript.preferences.preferTypeOnlyAutoImports": true,
	"typescript.preferences.useAliasesForRenames": false,
	"typescript.referencesCodeLens.enabled": true,
	"typescript.referencesCodeLens.showOnAllFunctions": true,
	"typescript.suggest.autoImports": true,
	"typescript.suggest.classMemberSnippets.enabled": true,
	"typescript.suggest.completeFunctionCalls": true,
	"typescript.suggest.includeAutomaticOptionalChainCompletions": true,
	"typescript.suggest.objectLiteralMethodSnippets.enabled": true,
	"typescript.suggest.paths": true,
	"typescript.tsserver.experimental.enableProjectDiagnostics": true,
	"typescript.updateImportsOnPaste.enabled": true,
	"typescript.workspaceSymbols.scope": "currentProject"
}
