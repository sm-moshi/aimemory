# Exclude version control and editor config
.git/
.vscode/

# Exclude AI and memory bank data
.cursor/
memory-bank/

# Exclude tests and docs
*.test.*
test/
docs/
docs/maybe/
docs/wip/
docs/experimental/

# Exclude build artifacts and dependencies
node_modules/
# Allow dist/extension.cjs and dist/assets for runtime
!dist/extension.cjs
!dist/extension.js
!dist/cli.js
!dist/commandHandler.js
!dist/index.js
!dist/mcpServer.js
!dist/mcpServerCli.js
!dist/memoryBank.js
!dist/memoryBankCore.js
!dist/memoryBankServiceCore.js
!dist/types.js
!dist/webviewManager.js
!dist/assets/**
!dist/lib/rules/**
!dist/webview/**
!dist/core/**
!dist/mcp/**
!dist/types/**
!dist/utils/**
!dist/*.js.map

# Exclude environment and log files
.env
*.log
*.DS_Store
logs/

# Exclude repomix and other local metadata
.repomix/

# Exclude any markdown rulesets (if not needed in npm)
*.mdc

# Exclude other common junk
Thumbs.db
npm-debug.log

# Allow esbuild.js for npm scripts
!esbuild.js

# Exclude test output and system files
dist/test/
dist/.DS_Store
