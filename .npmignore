# Exclude version control and editor config
.git/
.vscode/

# Exclude AI and memory bank data
.cursor/
memory-bank/

# Exclude tests and docs
*.test.*
test/
docs/

# Exclude build artifacts and dependencies
node_modules/
# Allow dist/extension.cjs and dist/assets for runtime
!dist/extension.cjs
!dist/assets/**
!dist/lib/rules/**
!dist/webview/**
!dist/cli.js
!dist/commandHandler.js
!dist/mcpServer*
!dist/memoryBank*
!dist/extension.js
!dist/types.ts
dist/*.js.map

# Exclude environment and log files
.env
*.log
*.DS_Store

# Exclude repomix and other local metadata
.repomix/

# Exclude any markdown rulesets (if not needed in npm)
*.mdc

# Exclude other common junk
Thumbs.db
npm-debug.log

# Allow esbuild.js for npm scripts
!esbuild.js
